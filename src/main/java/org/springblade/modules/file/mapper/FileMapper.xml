<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.modules.file.mapper.FileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="fileResultMap" type="org.springblade.modules.file.entity.File">
        <id column="id" property="id"/>
        <result column="foreign_id" property="foreignId"/>
        <result column="category" property="category"/>
        <result column="type" property="type"/>
    </resultMap>


    <select id="selectFilePage" resultMap="fileResultMap">
        select * from file where is_deleted = 0
    </select>

</mapper>
